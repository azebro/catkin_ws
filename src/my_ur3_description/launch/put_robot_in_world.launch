<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <!-- send robot urdf to param server -->
  <param name="robot_description" command="$(find xacro)/xacro '$(find my_ur3_description)/robots/ur3_robot.urdf.xacro'" />

  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot -z 0.3 -J shoulder_pan_joint 0.0 -J shoulder_lift_joint 0.0 -J elbow_joint 0.0 -J wrist_1_joint 0.0 -J wrist_2_joint 0.0 -J wrist_3_joint 0.0 -unpause" respawn="false" output="screen" />

  <!-- Add controller launch file here -->
<include file="$(find my_ur3_description)/launch/ur3_control.launch"/>

</launch>

